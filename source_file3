int add(int a, int b) {
    return a + b;
}

int main() {
    int x = 0;
    float y = 3.14;
    int z = add(3, 4);

    for (x = 0; x < 5; x = x + 1) {
        if (x == 2) {
            continue;
        } else {
            y = y + 1;
        }
    }

    while (x > 0) {
        if (x == 3) {
            break;
        }
        x = x - 1;
    }

    do {
        y = y - 1;
    } while (y > 0);

    return 0;
}
